
<%- layout('boilerplate') -%>
<% if (error.length>0) { %>
    <div class="row">
      <div class="col-sm-6 offset-sm-3">
        <div class="alert alert-danger" role="alert">
          <h4 class="alert-heading"><%= error %></h4>
          <% if (process.env.NODE_ENV !== "production") {%>
          <p><%= error.stack %></p>
          <% } %>
        </div>
      </div>
    </div>
<% } %>